pipeline {
    agent any
    stages {
        stage('checkout'){
            steps{
                git 'https://github.com/pratyush-shishodiya/django_docker_postgres.git'
            }
        }
        stage('Build') {
            steps {
                echo 'docker compose running'
                sh 'docker-compose up -d'
            }
        }
        stage('Test') {
            steps {
                // Your test steps here
                echo 'Testing done'
            }
        }
        stage('Deploy') {
            steps {
                // Your deployment steps here
                echo 'Deployed'
            }
        }
    }
}


